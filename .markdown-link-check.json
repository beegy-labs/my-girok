{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 2,
  "fallbackRetryDelay": "5s",
  "aliveStatusCodes": [200, 206]
}
