{
  "name": "@my-girok/types",
  "version": "0.1.0",
  "description": "Shared TypeScript types for my-girok",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./identity": {
      "types": "./dist/identity/index.d.ts",
      "import": "./dist/identity/index.js",
      "default": "./dist/identity/index.js"
    }
  },
  "scripts": {
    "build": "tsc && cp -r src/generated dist/",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.11.0",
    "@protobuf-ts/runtime-rpc": "^2.11.0"
  },
  "devDependencies": {
    "typescript": "catalog:"
  }
}
