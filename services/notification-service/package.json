{
  "name": "@my-girok/notification-service",
  "version": "0.1.0",
  "description": "Push notification and SMS service for sending notifications via gRPC",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,test}/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:e2e": "vitest run --config ./vitest-e2e.config.ts",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:pull": "prisma db pull",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.6",
    "@grpc/proto-loader": "^0.7.15",
    "@my-girok/nest-common": "workspace:*",
    "@my-girok/types": "workspace:*",
    "@nestjs/common": "catalog:",
    "@nestjs/config": "catalog:",
    "@nestjs/core": "catalog:",
    "@nestjs/microservices": "catalog:",
    "@nestjs/platform-express": "catalog:",
    "@nestjs/schedule": "catalog:",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.5.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/sdk-trace-node": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.30.0",
    "@prisma/client": "catalog:",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "express": "^5.2.1",
    "firebase-admin": "^13.0.2",
    "helmet": "catalog:",
    "joi": "^18.0.2",
    "kafkajs": "catalog:",
    "prisma": "catalog:",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@my-girok/vitest-config": "workspace:*",
    "@nestjs/cli": "catalog:",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "catalog:",
    "@types/express": "catalog:",
    "@types/node": "catalog:",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "eslint": "catalog:",
    "globals": "^16.5.0",
    "supertest": "^7.1.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "catalog:",
    "unplugin-swc": "^1.5.4",
    "vitest": "catalog:",
    "webpack-node-externals": "^3.0.0"
  }
}
