# Analytics Service Environment Variables

# Server
PORT=3004
NODE_ENV=development

# ClickHouse
CLICKHOUSE_HOST=clickhouse
CLICKHOUSE_PORT=8123
CLICKHOUSE_DATABASE=analytics_db
CLICKHOUSE_USERNAME=analytics_user
CLICKHOUSE_PASSWORD=
CLICKHOUSE_ASYNC_INSERT=true
CLICKHOUSE_WAIT_FOR_ASYNC_INSERT=false

# JWT (for token validation)
JWT_SECRET=your-jwt-secret-key

# Valkey Cache (Redis-compatible)
VALKEY_HOST=localhost
VALKEY_PORT=6379
VALKEY_PASSWORD=
VALKEY_DB=4

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100

# OTEL (OpenTelemetry)
OTEL_EXPORTER_OTLP_ENDPOINT=http://otel.girok.dev:4318
OTEL_SERVICE_NAME=analytics-service
